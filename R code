deaths<- united_states_covid19_cases_deaths_and_testing_by_state_states_only
govenors<- US_govenors_party_2020
age<-US_state_median_population
gini<-gini_index

deaths<- united_states_covid19_cases_deaths_and_testing_by_state_states_only[c(1,7)]

final1<- merge(deaths, govenors, by = "State")

final2 <- merge(gini, age, by = "State")

final <- merge(final1, final2, by = "State")

jointdataset(deaths,govenors, by = "State")

install.packages("writexl")

library(writexl)

write_xlsx(x = final, path = "final.xlsx")
